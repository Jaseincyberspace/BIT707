CREATE TABLE Task (
	taskNumber INTEGER PRIMARY KEY,
	taskName TEXT NOT NULL,
    taskDescription TEXT,
    dueDate TEXT CHECK(dueDate is null or length(dueDate) == 10),
    status TEXT NOT NULL CHECK(status == "true" or status == "false")
);

